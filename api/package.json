{
  "name": "resume-api-server",
  "version": "1.0.0",
  "description": "Resume API Playground Backend with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "seed": "ts-node src/seed.ts",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
